{
  "compilerOptions": {
    "declaration": false,
    "module": "es6",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "sourceMap": false,
    "lib": ["dom", "es6"],
    "rootDirectory": "./src"
  },
  "exclude": [
    "node_modules"
  ]
}
